{
    "name": "cpppp",
    "displayName": "CPPPP",
    "description": "吃一个月的傲娇鸡，谢谢",
    "version": "0.2.0",
    "publisher": "lisnb",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.addHeaderGuard",
        "onCommand:extension.genGet",
        "onCommand:extension.genSet",
        "onCommand:extension.genGetAndSet",
        "onCommand:extension.GenClear"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [ {
            "command": "extension.addHeaderGuard",
            "title": "CPPPP Add Header Guard"
        }, {
            "command": "extension.genGet",
            "title": "CPPPP Gen Get"
        }, {
            "command": "extension.genSet",
            "title": "CPPPP Gen Set"
        }, {
            "command": "extension.genGetAndSet",
            "title": "CPPPP Gen Get And Set"
        }, {
            "command": "extension.GenClear",
            "title": "CPPPP Gen Clear"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
